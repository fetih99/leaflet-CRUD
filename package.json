{
  "name": "leaflet-crud",
  "version": "1.1.0",
  "description": "A Rollup + Bulma-based Leaflet.JS CRUD Implementation for PostgreSQL/PostGIS",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyprasetya/leaflet-CRUD.git"
  },
  "keywords": [
    "rollup",
    "bulma"
  ],
  "author": "andyprasetya@opense7en.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyprasetya/leaflet-CRUD/issues"
  },
  "homepage": "https://github.com/andyprasetya/leaflet-CRUD#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "autoprefixer": "latest",
    "node-sass": "latest",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.16.2",
    "rollup-plugin-iife": "^0.3.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "latest",
    "rollup-plugin-terser": "^5.0.0",
    "serve": "^11.0.2"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "crypto-js": "latest",
    "date-fns": "latest",
    "leaflet": "latest",
    "leaflet-draw": "latest",
    "leaflet-groupedlayercontrol": "latest",
    "leaflet.control.layers.tree": "latest",
    "numeral": "latest",
    "terraformer": "latest",
    "terraformer-wkt-parser": "latest"
  }
}
